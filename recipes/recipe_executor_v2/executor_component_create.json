{
  "steps": [
    {
      "type": "read_file",
      "path": "recipes/recipe_executor_v2/specs/docs/registry.md",
      "artifact": "registry_docs"
    },
    {
      "type": "read_file",
      "path": "recipes/recipe_executor_v2/specs/docs/context.md",
      "artifact": "context_docs"
    },
    {
      "type": "execute_recipe",
      "recipe_path": "recipes/recipe_executor_v2/project_component_template.json",
      "context_overrides": {
        "component_id": "executor",
        "existing_code": "{{existing_code}}",
        "additional_content": "<REGISTRY_DOCS>\n{{registry_docs}}\n</REGISTRY_DOCS>\n<CONTEXT_DOCS>\n{{context_docs}}\n</CONTEXT_DOCS>"
      }
    }
  ]
}
