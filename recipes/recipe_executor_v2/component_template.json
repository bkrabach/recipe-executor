{
  "steps": [
    {
      "type": "read_file",
      "path": "recipes/recipe_executor_v2/specs/{{component_id}}_component_spec.md",
      "artifact": "component_spec"
    },
    {
      "type": "execute_recipe",
      "recipe_path": "recipes/recipe_executor_v2/generate_code.json",
      "context_overrides": {
        "spec": "{{component_spec}}",
        "component_name": "{{component_name}}",
        "component_description": "{{component_description}}",
        "output_file_path": "recipe_executor/{{component_id}}.py",
        "model": "openai:o3-mini",
        "output_root": "output/recipe_executor",
        "existing_code_text": "{{existing_code_text}}",
        "existing_code_section": "{{existing_code_section}}",
        "additional_content": "{{additional_content}}"
      }
    }
  ]
}
